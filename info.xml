<?xml version="1.0"?>
<extension key="searchkitpivot" type="module">
  <file>searchkitpivot</file>
  <name>SearchKit pivot table display</name>
  <description>Pivot table for SearchKit using library pivotjs</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>syxys</author>
    <email>info@sylvain-plante.me</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">http://FIXME</url>
    <url desc="Documentation">http://FIXME</url>
    <url desc="Support">http://FIXME</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2024-03-14</releaseDate>
  <version>1.01</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>5.70</ver>
  </compatibility>
  <comments>This is a new, alpha module</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Searchkitpivot</namespace>
    <format>23.02.1</format>
    <angularModule>crmSearchkitpivot</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
    <mixin>ang-php@1.0.0</mixin>
    <mixin>scan-classes@1.0.0</mixin>
  </mixins>
</extension>
